{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "name": "address"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "default": "address",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "zipCode": {
      "type": "string"
    }
  }
}
